import { OnInit, OnDestroy, EventEmitter, ChangeDetectorRef, ElementRef, OnChanges } from '@angular/core';
export declare class NgxNavDrawerComponent implements OnChanges, OnDestroy, OnInit {
    private platformId;
    private changeDetectorRef;
    openChange: EventEmitter<boolean>;
    open: boolean;
    fixed: boolean;
    fixedAtWidth: string;
    nav: ElementRef;
    show: boolean;
    fixedMode: boolean;
    private subscription;
    private lastFocusedElement;
    constructor(platformId: any, changeDetectorRef: ChangeDetectorRef);
    ngOnInit(): void;
    rootClick(event: any): void;
    ngOnChanges(): void;
    ngOnDestroy(): void;
    outerClick(event: any): void;
    setFixedPosition(): void;
    toggle(): void;
    focus(): void;
}
